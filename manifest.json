{
  "manifest_version": 2,
  "name": "Browser Project",
  "version": "0.1",

  "description": "experimental browser",
  "icons": {
    "128": "logo128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webRequest",
    "storage",
    "webview",
    "audioCapture",
    "videoCapture",
    "fullscreen",
    "pointerLock",
    "http://*/*",
    "https://*/*"
    ]
}
